{
    "name": "netusco/cakephp3-chartjs",
    "description": "ChartJs plugin for CakePHP 3.0",
    "type": "cakephp-plugin",
    "require": {
            "php": ">=5.4",
            "cakephp/cakephp": "~3.0",
            "nnnick/chartjs": "~1"
        },
    "require-dev": {
            "phpunit/phpunit": "*"
        },
    "autoload": {
            "psr-4": {
                        "ChartJs\\": "src"
                    }
        },
    "autoload-dev": {
            "psr-4": {
                        "ChartJs\\Test\\": "tests",
                        "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
                    }
        }
}
